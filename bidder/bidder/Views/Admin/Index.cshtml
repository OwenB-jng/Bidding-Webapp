@{
    ViewData["Title"] = "Admin Page";
}
<div class="Admin">
    <div>
        <table>
            <tr>
                <th>Item Name</th>
                <th>Item Description</th>
                <th>Starting Bid</th>
                <th>End Time</th>
                <th>Condition</th>
                <th>Type</th>
                <th>Image</th>
            </tr>
            <tbody>
                @foreach (Auction a in Model)
                {
                    <tr>
                        <td><hr></td>
                        <td><hr></td>
                        <td><hr></td>
                        <td><hr></td>
                        <td><hr></td>
                        <td><hr></td>
                        <td><hr></td>
                    </tr>
                    <tr>
                        <td>@a.itemName</td>
                        <td>@a.itemDescription</td>
                        <td>@a.startingBid</td>
                        <td>@a.endTime</td>
                        <td>@a.condition</td>
                        <td>@a.type</td>
                        <td><img src="@a.image" width="100" height="100"></td>
                    </tr>

                }
            </tbody>

        </table>
    </div>
    <hr>
    <div>
        <table>
            <tr>
                <th>Item Name</th>
